@prefix gaf: <http://groundedannotationframework.org/gaf#> .
@prefix grasp: <http://groundedannotationframework.org/grasp#> .
@prefix leolaniFriends: <http://cltl.nl/leolani/friends/> .
@prefix leolaniTalk: <http://cltl.nl/leolani/talk/> .
@prefix leolaniTime: <http://cltl.nl/leolani/date/> .
@prefix leolaniWorld: <http://cltl.nl/leolani/world/> .
@prefix n2mu: <http://cltl.nl/leolani/n2mu/> .
@prefix ns1: <urn:x-rdflib:> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sem: <http://semanticweb.cs.vu.nl/2009/11/sem/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix time: <http://www.w3.org/TR/owl-time/#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xml1: <https://www.w3.org/TR/xmlschema-2/#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

leolaniTalk:perspectives {
    leolaniTalk:chat_turn_char a gaf:Mention ;
        gaf:containsDenotation leolaniWorld:,
            leolaniWorld:spicy_food ;
        gaf:denotes leolaniWorld:_likes_spicy_food ;
        grasp:hasAttribution leolaniTalk:chat_turn_char_CERTAIN ;
        grasp:wasAttributedTo leolaniFriends:Bram .

    leolaniTalk:chat_turn_char_CERTAIN a grasp:Attribution ;
        grasp:isAttributionFor leolaniTalk:chat_turn_char ;
        rdf:value grasp:CERTAIN .
}

leolaniWorld:instances {
    leolaniFriends:Bram a gaf:Instance,
            sem:Actor ;
        rdfs:label "Bram" .

    leolaniWorld: a gaf:Instance,
            owl:Thing ;
        rdfs:label "" ;
        gaf:denotedIn leolaniTalk:chat_turn_char .

    leolaniWorld:_likes_spicy_food a gaf:Instance,
            grasp:Statement ;
        grasp:denotedBy leolaniTalk:chat_turn_char .

    leolaniWorld:spicy_food a gaf:Instance,
            owl:Thing ;
        rdfs:label "spicy food" ;
        gaf:denotedIn leolaniTalk:chat_turn_char .
}

{
    leolaniTalk:chat a sem:Event ;
        sem:hasActor leolaniFriends:Bram ;
        sem:hasSubevent leolaniTalk:chat_turn ;
        sem:hasTime <http://cltl.nl/leolani/date/2018-03-18> .

    leolaniTalk:chat_turn a sem:Event ;
        sem:hasActor leolaniFriends:Bram ;
        sem:hasTime <http://cltl.nl/leolani/date/2018-03-18> .

    <http://cltl.nl/leolani/date/2018-03-18> a time:DateTimeDescription ;
        time:day "18"^^xml1:gDay ;
        time:month "3"^^xml1:gMonthDay ;
        time:unitType time:unitDay ;
        time:year "2018"^^xml1:gYear .
}

leolaniWorld:_likes_spicy_food {
    leolaniWorld: n2mu:likes leolaniWorld:spicy_food .
}

