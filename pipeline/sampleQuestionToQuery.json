Who is from Mexico


{
  "subject": {
    "id": "",
    "label": "",
    "type": "Person"
  },
  "predicate": {
    "type": "isFrom"
  },
  "object": {
    "id": "uri",
    "label": "Mexico",
    "type": "Location"
  },
  "response": {
    "role": "subject",
    "format": "list"
}

  SELECT ?s ?slabel ?authorlabel
        WHERE
        ?s isFrom ?o
        ?s rdf:type n2mu:Person
        ?o rdfs:label "Mexico"
        ?o rdf:type n2mu:Place       ## add only if we have this information
        ?s rdfs:label ?slabel

        GRAPH ?g { ?s isFrom ?o . }
        ?g grasp:denotedBy ?m .
        ?a grasp:isAttributionFor ?m .
        ?a grasp:wasDerivedFrom ?turn .
        ?turn prov:wasAttributedTo ?author .
        ?author rdfs:label ?authorlabel
}

