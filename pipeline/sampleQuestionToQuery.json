Is Selene from Mexico


{
  "subject": {
    "id": "",
    "label": "Selene",
    "type": "Person"
  },
  "predicate": {
    "type": "isFrom"
  },
  "object": {
    "id": "",
    "label": "Mexico",
    "type": "Location"
  },
  "response": {
    "role": "",
    "format": "bool"
}

  SELECT ?authorlabel ?v
        WHERE
        ?s isFrom ?o
        ?s rdf:type n2mu:Person      ## add if we have this information, if we dont have we infer from range of property'
        ?s rdfs:label "Selene"

        ?o rdfs:label "Mexico"
        ?o rdf:type n2mu:Place       ## add only if we have this information

        GRAPH ?g { ?s isFrom ?o . }
        ?g grasp:denotedBy ?m .
        ?a grasp:isAttributionFor ?m .
        ?a rdf:value ?v .
        ?a grasp:wasDerivedFrom ?turn .
        ?turn prov:wasAttributedTo ?author .
        ?author rdfs:label ?authorlabel
}

